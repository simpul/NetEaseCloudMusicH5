
@font-face {
	font-family: 'iconfont';  /* project id 1354255 */
	src: url(data:application/vnd.ms-fontobject;base64,);
	src: url(data:application/vnd.ms-fontobject;base64,?#iefix) format('embedded-opentype'),
	url(data:font/woff2;base64,) format('woff2'),
	url(data:font/woff;base64,) format('woff'),
	url(data:font/ttf;base64,) format('truetype'),
	url(../../static/img/iconfont.cfdc7be0.svg#iconfont) format('svg');
}
.iconfont {
	font-family:"iconfont" !important;
	font-size:48rpx;
	font-style:normal;
	color: #ccc;
	-webkit-font-smoothing: antialiased;
	-webkit-text-stroke-width: 0.4rpx;
	-moz-osx-font-smoothing: grayscale;
}
.cover {
	position: relative;
}
.cover ._img {
	position: absolute;
	top: 15%;
	left: 0;
	right: 0;
	width: 60%;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	margin: auto;
	border: 8rpx solid rgba(255, 255, 255, .3);
}
.btns {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	width: 100%;
	-webkit-justify-content: space-around;
	    -ms-flex-pack: distribute;
	        justify-content: space-around;
	position: absolute;
	bottom: 0;
	left: 0;
}


.lyrics {
	overflow: hidden;
}
.nolyric {
	color: #fff;
	text-align: center;
	padding-top: 50%;
}
.havelyric ._ul {
	padding-top: 80%;
	-webkit-transition: .5s;
	-o-transition: .5s;
	transition: .5s;
}
.havelyric ._ul ._li {
	text-align: center;
	color: #999;
	height: 120rpx;
	padding: 0 40rpx;
}
.active {
	color: #fff!important;
}


.fade-enter-active, .fade-leave-active {
	-webkit-transition: opacity .1s;
	-o-transition: opacity .1s;
	transition: opacity .1s;
}
.fade-enter, .fade-leave-to {
	opacity: 0;
}
.player {
	height: 100vh;
	background-color: rgba(0, 0, 0, .5);
	-webkit-transition: 1s;
	-o-transition: 1s;
	transition: 1s;
}
.deeper {
	background-color: rgba(0, 0, 0, .6)!important;
}
.status_bar {
	height: var(--status-bar-height);
	width: 100%;
}
.comp {
	height: -webkit-calc(100vh - 340rpx);
	height: calc(100vh - 340rpx);
}
.mask {
	position: fixed;
	-webkit-filter: blur(50rpx);
	filter: blur(50rpx);
	width: 200vw;
	height: 100vh;
	top: 0;
	left: -50vw;
	z-index: -1;
}

